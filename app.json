{
  "expo": {
    "name": "Rádio Animu",
    "slug": "Rádio Animu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_top.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_top.png",
      "backgroundColor": "#270051"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-top.png",
        "backgroundColor": "#270051"
      },
      "package": "com.nessjs.animu"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "506eccd6-2206-459c-b70b-5b79515d5b49"
      }
    }
  }
}
