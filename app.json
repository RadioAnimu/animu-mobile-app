{
  "expo": {
    "name": "RÃ¡dio Animu",
    "slug": "animu",
    "orientation": "portrait",
    "icon": "./assets/icon_top.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash_top.png",
      "backgroundColor": "#270051"
    },
    "scheme": "animuapp",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nessjs.animu",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "processing"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-top.png",
        "backgroundColor": "#270051"
      },
      "package": "com.nessjs.animu",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/proximanova-reg.ttf",
            "./src/assets/fonts/proximanova-light.ttf",
            "./src/assets/fonts/proximanova-bold.ttf",
            "./src/assets/fonts/ProximaNova-Black.ttf"
          ]
        }
      ],
      "expo-asset",
      "expo-background-task",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "506eccd6-2206-459c-b70b-5b79515d5b49"
      }
    }
  }
}